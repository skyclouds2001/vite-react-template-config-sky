  "contributors": [
    "skyclouds2001"
  ],
  "main": "src/main.tsx",
  "packageManager": "pnpm@8.4.0",
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=9.6.0",
    "yarn": ">=1.22.0",
    "pnpm": ">=8.4.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint && pnpm lint:markdownlint && pnpm lint:type",
    "lint:eslint": "eslint --fix --cache --cache-location \"./node_modules/.eslint/.eslintcache\" .",
    "lint:prettier": "prettier --write --cache --cache-location \"./node_modules/.prettier/.prettiercache\" .",
    "lint:stylelint": "stylelint --fix --cache --cache-location \"./node_modules/.stylelint/.stylelintcache\"",
    "lint:markdownlint": "markdownlint --fix **/*.md",
    "lint:type": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "cross-env NODE_ENV=test vitest",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "clean": "rimraf node_modules && rimraf dist",
    "changeset": "changeset",
    "bump": "changeset version",
    "commit": "cz",
    "prepare": "is-ci || husky install",
    "preinstall": "only-allow pnpm"
  },
  "dependencies": {
