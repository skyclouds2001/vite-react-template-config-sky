    "markdownlint": "^0.31.1",
    "markdownlint-cli": "^0.37.0",
    "only-allow": "^1.2.0",
    "pnpm": "8.8.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
